<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.0//EN"
"http://struts.apache.org/dtds/struts-2.0.dtd">

<struts>
    <include file="example.xml"/>
    <!-- Configuration for the default package. -->
    <package name="default" namespace="/Views" extends="struts-default">
        <action name="parvez"   class="com.parvez.Login">
            <result name="success">/Views/Welcome.jsp</result>
              <result name="error">/Views/Error.jsp</result>
           
        </action>
         <action name="regi"  method="smoke" class="com.parvez.Login">
            <result name="success">/Views/reg.jsp</result>
             <result name="error">/Views/Welcome.jsp</result>
           
                </action>
        
         <action name="atiqe"  class="com.parvez.Reg">
            <result name="success">/Views/reg.jsp</result>
             <result name="error">/Views/Error.jsp</result>
           
        </action>
        
        
    </package>
</struts>
